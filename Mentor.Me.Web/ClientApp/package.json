{
    "name": "mentor.me.web",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "prestart": "node aspnetcore-https",
        "start:dev": "ng serve",
        "start": "run-script-os",
        "start:windows": "ng serve --port 44408 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
        "start:default": "ng serve --port 44408 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "build:ssr": "ng run Mentor.Me.Web:server:dev",
        "watch": "ng build --watch --configuration development",
        "test": "ng test"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/schematics": "^13.0.2",
        "@angular-eslint/schematics": "^13.0.0-alpha.0",
        "@angular/animations": "~13.0.2",
        "@angular/cdk": "^13.0.2",
        "@angular/common": "~13.0.2",
        "@angular/compiler": "~13.0.2",
        "@angular/core": "~13.0.2",
        "@angular/forms": "~13.0.2",
        "@angular/material": "^13.0.2",
        "@angular/platform-browser": "~13.0.2",
        "@angular/platform-browser-dynamic": "~13.0.2",
        "@angular/platform-server": "~13.0.2",
        "@angular/router": "~13.0.2",
        "@microsoft/signalr": "^6.0.0",
        "angular-responsive-carousel": "^2.1.2",
        "aos": "^2.3.4",
        "bootstrap": "^5.1.0",
        "jquery": "^3.5.1",
        "oidc-client": "^1.11.5",
        "popper.js": "^1.16.0",
        "run-script-os": "^1.1.6",
        "rxjs": "~6.6.0",
        "tslib": "^2.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.0.3",
        "@angular-eslint/eslint-plugin": "^12.6.1",
        "@angular/cli": "~13.0.3",
        "@angular/compiler-cli": "~13.0.2",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "codelyzer": "^6.0.0",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-indent-class-properties": "^1.0.3",
        "eslint-plugin-unused-imports": "1.1",
        "jasmine-core": "~3.8.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "typescript": "~4.4.4"
    }
}
